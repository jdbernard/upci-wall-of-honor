{
  "name": "upci-wall-of-honor-api",
  "version": "1.1.1-alpha",
  "description": "UPCI Wall of Honor API.",
  "main": "openapi/openapi.yaml",
  "scripts": {
    "clean": "rm -r dist; rm lambda/verify_jwt.zip",
    "build": "npm run build-api-spec && npm run build-verify_jwt",
    "build-api-spec": "mkdir dist; npx openapi bundle openapi/openapi.yaml > dist/openapi.yaml",
    "build-verify_jwt": "(cd lambda/verify_jwt && npm install && zip -r ../verify_jwt.zip .)",
    "test": "npx openapi lint openapi/openapi.yaml"
  },
  "repository": {
    "type": "git",
    "url": "https://git.jdb-labs.com/jdb-labs/upci-wall-of-honor.git"
  },
  "author": "Jonathan Bernard <jonathan@jdbernard.com>",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@redocly/openapi-cli": "^1.0.0-beta.9"
  },
  "dependencies": {}
}
